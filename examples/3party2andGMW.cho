-- Read Secrets
c_in = SECRET @P1
h1_in = SECRET @P2
h2_in = SECRET @P2

-- Set up shares
c_s1 = FLIP @P1
c_s2 = c_s1 + c_in
h1_s1 = FLIP @P2
h1_s2 = h1_s1 + h1_in
h2_s1 = FLIP @P2
h2_s2 = h2_s1 + h2_in

-- Round one messages
SEND c_s2 TO P2
SEND h1_s1 TO P1
SEND h2_s1 TO P1

